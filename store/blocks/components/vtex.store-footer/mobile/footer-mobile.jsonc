{
    "flex-layout.row#footer-mobile": {
        "children": [
            "flex-layout.col#footer-mobile",
            "flex-layout.row#footer-copyright-container"
        ],
        "props": {
            "blockClass": "footer-container-mobile"
        }
    },
    "flex-layout.col#footer-mobile": {
        "children": [
            "image#footer-col-1__logo",
            "flex-layout.row#social",
            "rich-text#footer-col-2__text-2",
            "rich-text#footer-col-2__text-1",
            "rich-text#footer-col-2__text-3",
            "link#footer-regret-button",
            "disclosure-layout-group#group",
            "newsletter-form#footer-newsletter"
        ],
        "props": {
            "blockClass": "footer-col-1",
            "preventVerticalStretch": true
        }
    },

    "disclosure-layout-group#group": {
        "children": [
            "disclosure-layout#first",
            "disclosure-layout#second",
            "disclosure-layout#thirth",
            "disclosure-layout#fourth",
            "disclosure-layout#fifth",
            "disclosure-layout#sixth"
        ],
        "props": {
            "animated": true
        }
    },

    // FIRST
    "disclosure-layout#first": {
        "children": ["disclosure-trigger#first", "disclosure-content#first"]
    },
    "disclosure-trigger#first": {
        "children": ["disclosure-state-indicator", "rich-text#first-title-1"],
        "props": {
            "blockClass": "question",
            "as": "div"
        }
    },
    "disclosure-content#first": {
        "children": [
            "rich-text#first-content-1",
            "rich-text#first-content-2",
            "rich-text#first-content-3"
        ]
    },
    "rich-text#first-title-1": {
        "props": {
            "text": "Comprar en marketon",
            "blockClass": "footer-dropdown__title"
        }
    },
    "rich-text#first-content-1": {
        "props": {
            "text": "[Servicio técnico]()",
            "blockClass": "footer-dropdown__content"
        }
    },
    "rich-text#first-content-2": {
        "props": {
            "text": "[Protección de usuarios]()",
            "blockClass": "footer-dropdown__content"
        }
    },
    "rich-text#first-content-3": {
        "props": {
            "text": "[Financieros]()",
            "blockClass": "footer-dropdown__content"
        }
    },

