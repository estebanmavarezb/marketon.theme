    // ARROW
    "disclosure-state-indicator": {
        "props": {
            "Show": "icon-caret#down",
            "Hide": "icon-caret#up",
            "blockClass": "footer-dropdown__indicator"
        }
    },
    "icon-caret#down": {
        "props": {
            "orientation": "down",
            "blockClass": "footer-dropdown-indicator__arrow"
        }
    },
    "icon-caret#up": {
        "props": {
            "orientation": "up",
            "blockClass": "footer-dropdown-indicator__arrow"
        }
    }
